<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vBox" alignment="center" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="ResultController">
    <Label fx:id="titleLabel" text="Student Result" />
    <TableView fx:id="resultTable" onSort="#initialize">
        <columns>
            <TableColumn fx:id="subjectColumn" text="Subject" />
            <TableColumn fx:id="gradeColumn" text="Grade" />
        </columns>
    </TableView>
</VBox>
