<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CourseSelectionController">
    <children>
        <ComboBox fx:id="courseComboBox" layoutX="116.0" layoutY="58.0" prefWidth="150.0" promptText="Select Course" />
        <Button fx:id="selectButton" layoutX="166.0" layoutY="124.0" mnemonicParsing="false" onAction="#selectCourse" text="Select" />
    </children>
</AnchorPane>
